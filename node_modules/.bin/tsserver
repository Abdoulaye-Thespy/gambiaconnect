#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/thecto/FreelanceConnect/GambiaConnect/Code/node_modules/typescript/bin/node_modules:/home/thecto/FreelanceConnect/GambiaConnect/Code/node_modules/typescript/node_modules:/home/thecto/FreelanceConnect/GambiaConnect/Code/node_modules:/home/thecto/FreelanceConnect/GambiaConnect/node_modules:/home/thecto/FreelanceConnect/node_modules:/home/thecto/node_modules:/home/node_modules:/node_modules:/home/thecto/FreelanceConnect/GambiaConnect/Code/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/thecto/FreelanceConnect/GambiaConnect/Code/node_modules/typescript/bin/node_modules:/home/thecto/FreelanceConnect/GambiaConnect/Code/node_modules/typescript/node_modules:/home/thecto/FreelanceConnect/GambiaConnect/Code/node_modules:/home/thecto/FreelanceConnect/GambiaConnect/node_modules:/home/thecto/FreelanceConnect/node_modules:/home/thecto/node_modules:/home/node_modules:/node_modules:/home/thecto/FreelanceConnect/GambiaConnect/Code/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi
